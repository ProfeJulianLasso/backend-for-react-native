{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app": "<rootDir>/../src/application",
    "@domain": "<rootDir>/../src/domain",
    "@infra": "<rootDir>/../src/infrastructure"
  }
}
